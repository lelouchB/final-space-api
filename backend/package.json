{
    "name": "final-space-api-backend",
    "version": "0.0.0",
    "description": "Final Space API backend.",
    "main": "server.js",
    "scripts": {
        "dev": "nodemon server.js",
        "start": "node server.js",
        "prod": "NODE_ENV=production node server.js",
        "lint": "eslint .",
        "lint:fix": "eslint --fix --ext .js,.jsx .",
        "format": "prettier --write **/*.js",
        "seed": "node ./helpers/seeder.js"
    },
    "author": "Ashutosh K Singh",
    "license": "BSD-3-Clause",
    "dependencies": {
        "axios": "^0.20.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "express-rate-limit": "^5.1.3",
        "helmet": "^4.1.1",
        "mongoose": "^5.10.7",
        "morgan": "^1.10.0"
    },
    "devDependencies": {
        "dotenv": "^8.2.0",
        "eslint": "^7.10.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^6.12.0",
        "husky": "^4.3.0",
        "lint-staged": "^10.4.0",
        "nodemon": "^2.0.4",
        "prettier": "2.1.2"
    },
    "engines": {
        "node": "10.x"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lelouchB/final-space-api.git"
    },
    "keywords": [
        "api",
        "final space"
    ],
    "bugs": {
        "url": "https://github.com/lelouchB/final-space-api/issues"
    },
    "homepage": "https://github.com/lelouchB/final-space-api#readme",
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": "eslint --cache --fix",
        "*.{js,css,md}": "prettier --write"
    }
}
